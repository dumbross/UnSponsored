
{
    "manifest_version": 3,
    "name": "UnSponsored",
    "version": "1.0",
    "description": "Remove sponsored results from Google search.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.google.com/*"],
        "js": ["UnSponsored.js"]
      }
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "browser_action": {
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "options_ui": {
      "page": "popup.html"
    }
  }
